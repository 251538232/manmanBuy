    <script id="getCouponInfoTmp" type="text/html">
        {{each result as value i}}
        <div class="col-xs-4 col-sm-3 one">
            <div class="everycoupon">
                <!-- <p>{{value.couponTitle}}</p> -->
                <a href="./coupondetail.html?id={{value.couponId}}">
